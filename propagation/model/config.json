{
    "model_type": "TensorFlow",
    "model_algorithm": "lgb_propagation",
    "runtime": "python3.6",
    "apis": [{
        "protocol": "http",
        "url": "/",
        "method": "post",
        "request": {
            "Content-type": "multipart/form-data",
            "data": {
                "type": "object",
                "properties": {
                    "csv_file": {
                        "type": "file"
                    }
                }
            }
        },
        "response": {
            "Content-type": "multipart/form-data",
            "data": {
                "type": "object",
                "properties": {
                    "RSRP": {
                        "type": "array",
                        "items": [{
                            "type": "number"
                        }]
                    }
                }
            }
        }
    }],
	"dependencies": [{
		"installer": "pip",
		"packages": [
            {
                "package_name": "lightgbm",
                "package_version": "2.2.1",
                "restraint": "ATLEAST"
            },
            {
                "package_name": "scikit-learn",
                "package_version": "0.21.2",
                "restraint": "ATLEAST"
            },
            {
                "package_name": "pandas",
                "package_version": "0.25.1",
                "restraint": "ATLEAST"
            },
            {
                "package_name": "scipy",
                "package_version": "1.3.1",
                "restraint": "ATLEAST"
            },
            {
                "package_name": "numpy",
                "package_version": "1.17.2",
                "restraint": "ATLEAST"
            }
        ]
    }]
	
}

